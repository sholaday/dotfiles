[alias]
    l = log
    lg = log --graph --abbrev-commit --pretty=oneline
    s = status
    b = branch
    bd = branch -d
    a = add
    au = add -u
    d = diff
    p = pull
    ps = push
    c = commit
    ca = commit --amend
    ch = checkout
    n = checkout -b
    stl = stash list
    stps = stash push
    stpo = stash pop
    sta = stash apply
    std = stash drop
    cl = clean -df
    re = reset
    reh = reset --hard
    rb = rebase
    rbi = rebase -i
    rbo = rebase --onto
    pr = remote update -p
